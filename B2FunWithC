void copy(char * source, char * dest) {
  while (*dest++ = *source++); // using result of condition
}

int calc(int*digits, int length){
  int result = 0;
  for (int i = 0; i < length; i+=2){
    digits[i] = *(digits + i) << 1;
    if (digits[i] > 9)
            digits[i] -= 9;
  }
  for (int i = 0; i < length; i++) {
    result += digits[i];
  }
  return result % 10;
}
